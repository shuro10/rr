[
  {
    "name": "backend-container",
    "image": "700347967260.dkr.ecr.ap-northeast-1.amazonaws.com/cs-backend",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "backend",
        "awslogs-group": "/ecs/backend"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 3000,
        "hostPort": 3000
      }
    ],
    "environment": [
      {
        "name": "API_DOMAIN",
        "value": "https://cs-kent.ga"
      },
      {
        "name": "WORKDIR",
        "value": "app"
     },
      {
        "name": "CONTAINER_PORT",
        "value": "3000"
      },
      {
        "name": "RAILS_ENV",
        "value": "production"
      }
    ],
    "secrets": [
      {
        "name": "AWS_ACCESS_KEY_ID",
        "valueFrom": "RR_AWS_ACCESS_KEY_ID"
      },
      {
        "name": "AWS_SECRET_ACCESS_KEY",
        "valueFrom": "RR_SECRET_ACCESS_KEY"
      },
      {
        "name": "DATABASE_URL",
        "valueFrom": "RR_RAILS_MASTER_KEY"
      },
      {
        "name": "RAILS_MASTER_KEY",
        "valueFrom": "RR_RAILS_MASTER_KEY"
      }
    ]
  }
]